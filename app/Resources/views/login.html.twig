<?xml version="1.0"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
    <head>
        <meta charset="utf-8"/>
        <title>{% block title %}SEC - Panel de administraci√≥n{% endblock %}</title>

        {% stylesheets 
        '@SECSolicitudesBundle/Resources/public/css/login_base.css'
        '@SECSolicitudesBundle/Resources/public/css/login_skeleton.css'
        '@SECSolicitudesBundle/Resources/public/css/login_layout.css'

        %}
        <!--[if lt IE 9]>
        <link rel="stylesheet" href="css/ie.css" type="text/css" media="screen" />
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
        <link rel="stylesheet" media="screen,projection" type="text/css" href="{{ asset_url }}" />
    {% endstylesheets %} 

        {% javascripts
            '@SECSolicitudesBundle/Resources/public/js/jquery-1.8.2.min.js'
            '@SECSolicitudesBundle/Resources/public/js/app.js'
            
        %}
        <script src="{{ asset_url }}" type="text/javascript"></script>
        {% endjavascripts %}

        {% block js %}{% endblock %} 
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" /> 


            </head>


            <body>

                    
                    <!-- System Messages -->
                    {% block mensajes %}
                    
                        {% if app.session.hasFlash('success') %}
                            <h4 class="alert_success">
                            {{ app.session.flash('success') }}
                            </h4>
                        {% endif %}

                        {% if app.session.hasFlash('warning') %}
                            <h4 class="alert_warning">
                            {{ app.session.flash('warning') }}
                            </h4>
                        {% endif %}

                        {% if app.session.hasFlash('info') %}
                            <h4 class="alert_info">
                            {{ app.session.flash('info') }}
                            </h4>
                        {% endif %}

                        {% if app.session.hasFlash('error') %}
                                <h4 class="alert_error">
                                {{ app.session.flash('error') }}
                                </h4>
                        {% endif %}

                    {% endblock %}

                    {% block body %}{% endblock %}



            </body>
</html>